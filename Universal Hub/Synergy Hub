local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Synergy Hub - Universal", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- Section "Home"
local HomeTab = Window:MakeTab({
    Name = "Home",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

HomeTab:AddLabel("Creator: Xyraniz")
HomeTab:AddLabel("Discord User: xyraniz_official#0000")

HomeTab:AddButton({
    Name = "Copy Discord Link",
    Callback = function()
        setclipboard("https://discord.gg/ZdeHeXPFuP")
        print("Link copied to clipboard!")
    end
})


