local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt"))()

local win = lib:Window("Synergy Hub - Universal", Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)

-- Sección "Inicio"
local InicioTab = win:Tab("Inicio")

InicioTab:Label("Creador: Xyraniz")
InicioTab:Label("Usuario de Discord: xyraniz_official#0000")
InicioTab:Label("Ayudante: angel_xt_1#0000")

InicioTab:Button("Copiar Enlace Discord", function()
    setclipboard("https://discord.gg/ZdeHeXPFuP")
    print("¡Enlace copiado al portapapeles!")
end)

